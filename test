x = int(input("enter your x:"))
y = int(input("enter your y:"))
total = print(x, "+", y, "=", x + y)
subtraction = print(x, "-", y, "=", x - y)
beat = print(x, "*", y, "=", x * y)
power = print(x, "**", y, "=", x ** y)
division = print(x, "/", y, "=", x / y)
leftower = print(x, "%", y, "=", x % y)
[total, subtraction, beat, power, division, leftower] = int(input("enter your number"))
total = print("total=", total, sep="")
subtraction = print("subtraction=", subtraction, sep="")
beat = print("beat=", beat, sep="")
power = print("power=", power, sep="")
division = print("division=", division, sep="")
leftower = print("leftower=", leftower, sep="")
